<%= simple_form_for(@review) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :user %>
    <%= f.association :partner %>
    <%= f.input :title %>
    <%= f.input :body %>
    <%= f.input :engaged_as_app_exchange_partner %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
